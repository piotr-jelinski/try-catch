{
  "name": "@pjelinski/try-catch",
  "version": "1.0.1",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "build:check": "tsc --noEmit",
    "coverage": "jest --collect-coverage",
    "format": "prettier --write **/*.ts",
    "format:check": "prettier --check **/*.ts",
    "lint": "eslint **/*.ts --fix",
    "lint:check": "eslint **/*.ts",
    "prepublish": "npm run build",
    "test": "jest"
  },
  "keywords": [
